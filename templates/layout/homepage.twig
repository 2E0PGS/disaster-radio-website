{# START: Site Header #}
<header role="banner" class="site-header parallax" gumby-parallax="0.5">
  <div class="row">
    <div class="twelve columns header-intro-wrap fadeUpIn">
      <h1 class="logo">{{ site_name }}</h1>
      <p>{{ page.title }}</p>
      <div class="header-signup">
        <a href="/connect" class="btn-side btn-docs btn-grow">Help us grow</a>
      </div>
    </div>
  </div>
</header>
{# /END: Site Header #}

<main role="main" class="site-main-content">
  {{ body }}
</main>
